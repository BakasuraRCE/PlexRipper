{
	"name": "plex-ripper-web-ui",
	"version": "1.0.0",
	"description": "Plex Ripper Web UI",
	"author": "JasonLandbridge",
	"private": true,
	"scripts": {
		"dev": "nuxt-ts dev --no-inline",
		"build": "nuxt-ts build",
		"generate": "nuxt-ts build && nuxt-ts export",
		"generate-ts": "npx swagger-typescript-api -p http://localhost:5000/swagger/v1/swagger.json --no-client -o ./src/types/dto/ -n mainApi.ts",
		"start": "nuxt-ts start",
		"lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
		"lint-fix": "eslint . --ext .ts,.js,.vue --max-warnings=0 --fix & prettier-stylelint --ignore-path ./.eslintignore --write \"{,!(node_modules)/**/}*.{css,scss}\" --fix & prettier --ignore-path ./.eslintignore --write \"{,!(node_modules)/**/}*.{ts,vue,json,js,jsx}\"",
		"test": "jest"
	},
	"lint-staged": {
		"*.{js,vue}": "npm run lint",
		"*.{css,vue}": "stylelint"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"dependencies": {
		"@aspnet/signalr": "^1.1.4",
		"@mdi/font": "^5.6.55",
		"@nuxt/types": "^2.14.5",
		"@nuxt/typescript-runtime": "^2.0.0",
		"@nuxtjs/axios": "^5.12.2",
		"@nuxtjs/dotenv": "^1.4.0",
		"@ssv/signalr-client": "^2.0.0",
		"@types/classnames": "^2.2.10",
		"@types/node": "^14.10.3",
		"axios-observable": "^1.1.3",
		"classnames": "^2.2.6",
		"consola": "^2.15.0",
		"fluent-type-results": "^1.0.11",
		"lodash": "^4.17.20",
		"material-design-icons-iconfont": "latest",
		"moment-duration-format": "^2.3.2",
		"nuxt": "^2.14.5",
		"nuxt-i18n": "^6.15.4",
		"rxjs": "^6.6.3",
		"swagger-typescript-api": "^1.12.0",
		"vue": "^2.6.12",
		"vue-class-component": "^7.2.5",
		"vue-moment": "^4.1.0",
		"vue-property-decorator": "^9.0.0",
		"vue2-perfect-scrollbar": "^1.5.0"
	},
	"devDependencies": {
		"@nuxt/typescript-build": "^2.0.3",
		"@nuxtjs/eslint-config-typescript": "^3.0.0",
		"@nuxtjs/eslint-module": "^2.0.0",
		"@nuxtjs/stylelint-module": "^4.0.0",
		"@nuxtjs/vuetify": "^1.0.0",
		"@types/lodash": "^4.14.161",
		"@types/vue-moment": "^4.0.0",
		"@vue/test-utils": "^1.1.0",
		"babel-eslint": "^10.0.1",
		"babel-jest": "^24.1.0",
		"eslint": "^7.9.0",
		"eslint-config-prettier": "^6.10.0",
		"eslint-plugin-nuxt": "^1.0.0",
		"eslint-plugin-prettier": "^3.1.4",
		"husky": "^4.3.0",
		"jest": "^24.1.0",
		"lint-staged": "^10.4.0",
		"prettier": "^2.1.2",
		"stylelint": "^10.1.0",
		"ts-jest": "^25.0.0",
		"vue-jest": "^4.0.0-rc.0",
		"vuex-module-decorators": "^0.17.0"
	}
}
